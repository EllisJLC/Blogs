{{#each blogs as |blog| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <a href="/blog/{{blog.id}}">{{blog.name}}</a>
    </h2>
    <p>Created by {{blog.user.name}} on {{format_date blog.date_created}}</p>
    <br>
    
  </div>
  <div class="col-md-7">
    <h1>{{blog.title}}</h1>
    <p>{{blog.content}}</p>
  </div>
</div>
{{/each}}
